
public class NumericValues {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	    String str = "abcd1234567890pqr54897e8";
	    String a="";
	   for(int i=0;i<str.length();i++){
		   if(Character.isDigit(str.charAt(i))){
			   a=a+str.charAt(i);
			   
		   }
	   }
	   System.out.println(a);
	}
}
